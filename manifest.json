{
	"manifest_version": 2,
	"short_name": "SimplePass",
	"name": "SimplePass",
	"description": "A simple and secure password mananger",
	"version": "0.1.1",
	"permissions": [
		"storage",
		"tabs",
		"clipboardWrite"
	],
	"browser_action": {
		"default_icon": "images/icon128.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"content_scripts": []
}